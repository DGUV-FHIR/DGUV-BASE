<Bundle xmlns="http://hl7.org/fhir">
    <id value="100"/>
    <type value="collection" />
    <!--Ab hier werden die unfallabhängigen Diagnosen beschrieben-->
    <entry>
        <fullUrl value="https://fhir.dguv.de/Condition/Diagnose-1" />
        <resource>
            <Condition>
                <id value="Diagnose-1" />
                <meta>
                    <profile value="https://fhir.dguv.de/Basis/Condition/DGUV-Basis-PR-Unfalldiagnose" />
                </meta>
                <!--Frage: Ist die Jargondiagnose passend zum ICD-10 Code? Muss das beachtet werden. Was passiert wenn dies nicht passend ist? Welche Diagnose ist führend?-->
                <extension url="https://fhir.dguv.de/Basis/Extension/DGUV-Basis-EX-Jargondiagnose">
                    <valueString value="Schädelprellung"/>
                </extension>
                <code>
                    <coding>
                        <system value="http://fhir.de/CodeSystem/bfarm/icd-10-gm"/>
                        <code value="S06.0"/>
                        <display value="Gehirnerschütterung"/>
                    </coding>
                </code>
                <subject>
                    <reference value="Patient/111-222-333-444"/>
                </subject>
            </Condition>
        </resource>
    </entry>
    <entry>
        <fullUrl value="https://fhir.dguv.de/Condition/Diagnose-2" />
        <resource>
            <Condition>
                <id value="Diagnose-2" />
                <meta>
                    <profile value="https://fhir.dguv.de/Basis/Condition/DGUV-Basis-PR-Unfalldiagnose" />
                </meta>
                <!--Hier wieder die gleiche Jargondiagnose für eine ICD Schädel-Diagnose da im Bsp keine andere Jargondiagnose passt aber eie angegeben werden muss  -->
                <extension url="https://fhir.dguv.de/Basis/Extension/DGUV-Basis-EX-Jargondiagnose">
                    <valueString value="Schädelprellung"/>
                </extension>
                <code>
                    <coding>
                        <system value="http://fhir.de/CodeSystem/bfarm/icd-10-gm"/>
                        <code value="S06.70!"/>
                        <display value="Bewusstlosigkeit bei Schädel-Hirn-Trauma: Weniger als 30 Minuten"/>
                    </coding>
                </code>
                <subject>
                    <reference value="Patient/111-222-333-444"/>
                </subject>
            </Condition>
        </resource>
    </entry>
    <entry>
        <fullUrl value="https://fhir.dguv.de/Condition/Diagnose-3" />
        <resource>
            <Condition>
                <id value="Diagnose-3" />
                <meta>
                    <profile value="https://fhir.dguv.de/Basis/Condition/DGUV-Basis-PR-Unfalldiagnose" />
                </meta>
                <extension url="https://fhir.dguv.de/Basis/Extension/DGUV-Basis-EX-Jargondiagnose">
                    <valueString value="Rippenserienfraktur links 6. - 8. Rippe"/>
                </extension>
                <code>
                    <coding>
                        <extension url="http://fhir.de/StructureDefinition/seitenlokalisation">
                            <valueCoding>
                                <system value="https://fhir.kbv.de/CodeSystem/KBV_CS_SFHIR_ICD_SEITENLOKALISATION" />
                                <code value="L" />
                                <display value="links" />
                            </valueCoding>
                        </extension>
                        <system value="http://fhir.de/CodeSystem/bfarm/icd-10-gm"/>
                        <code value="S22.43"/>
                        <display value="Rippenserienfraktur: Mit Beteiligung von drei Rippen"/>
                    </coding>
                </code>
                <subject>
                    <reference value="Patient/111-222-333-444"/>
                </subject>
            </Condition>
        </resource>
    </entry>
</Bundle>