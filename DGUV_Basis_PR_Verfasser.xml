<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <url value="https://fhir.dguv.de/Basis/PractitionerRole/DGUV_Basis_PR_Verfasser" />
  <version value="1.1" />
  <name value="DGUV_Basis_PR_Verfasser" />
  <title value="Verfasser" />
  <status value="draft" />
  <date value="2024-01-31" />
  <description value="Dieses Profil bildet den Urheber des Inhaltes ab." />
  <fhirVersion value="4.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="PractitionerRole" />
  <baseDefinition value="https://fhir.dguv.de/Basis/PractitionerRole/DGUV-Basis-PR-Leistungserbringerrolle" />
  <derivation value="constraint" />
  <differential>
    <element id="PractitionerRole.identifier">
      <path value="PractitionerRole.identifier" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="type.coding.code" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="PractitionerRole.identifier:ANR">
      <path value="PractitionerRole.identifier" />
      <sliceName value="ANR" />
      <short value="Identifier-Profil für die Abbildung einer lebenslanger Arztnummer" />
      <definition value="Dieses Element dient der Darstellung der Arztnummer gemäß § 108 SGB V, die umgangssprachlich auch als Lebenslange Arztnummer (LANR) bezeichnet wird." />
      <max value="1" />
      <type>
        <code value="Identifier" />
        <profile value="http://fhir.de/StructureDefinition/identifier-lanr" />
      </type>
    </element>
    <element id="PractitionerRole.identifier:EFN">
      <path value="PractitionerRole.identifier" />
      <sliceName value="EFN" />
      <short value="Identifier-Profil für die Abbildung der Einheitlichen Fortbildungsnummer (EFN)" />
      <definition value="In diesem Element wird die einheitliche Fortbildungsnummer (EFN) dargestellt." />
      <max value="1" />
      <type>
        <code value="Identifier" />
        <profile value="http://fhir.de/StructureDefinition/identifier-zanr" />
      </type>
    </element>
    <element id="PractitionerRole.identifier:ZANR">
      <path value="PractitionerRole.identifier" />
      <sliceName value="ZANR" />
      <short value="Identifier-Profil für die Abbildung einer lebenslangen Zahnarztnummer" />
      <definition value="In diesem Element wird die Zahnarztnummer analog zur ANR abgebildet." />
      <max value="1" />
      <type>
        <code value="Identifier" />
        <profile value="http://fhir.de/StructureDefinition/identifier-zanr" />
      </type>
    </element>
    <element id="PractitionerRole.identifier:Telematik-ID">
      <path value="PractitionerRole.identifier" />
      <sliceName value="Telematik-ID" />
      <definition value="Bei der Telematik-ID handelt es sich um eine eindeutige elektronische Identität von Leistungserbringenden und medizinischen Institutionen in der Telematik-Infrastruktur." />
      <type>
        <code value="Identifier" />
        <profile value="http://fhir.de/StructureDefinition/identifier-telematik-id" />
      </type>
    </element>
    <element id="PractitionerRole.identifier:Institutionskennzeichen">
      <path value="PractitionerRole.identifier" />
      <sliceName value="Institutionskennzeichen" />
      <short value="Institutionskennzeichen (IKNR)" />
      <definition value="Als eindeutiges Merkmal zur Identifizierung von Vertragspartnern, die für die Sozialversicherungsträger Leistungen erbringen wird die IKNR durch die Arbeitsgemeinschaft Institutionskennzeichen vergeben." />
      <max value="0" />
      <type>
        <code value="Identifier" />
        <profile value="http://fhir.de/StructureDefinition/identifier-iknr" />
      </type>
    </element>
  </differential>
</StructureDefinition>